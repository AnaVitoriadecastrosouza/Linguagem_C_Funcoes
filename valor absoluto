#include <stdio.h>

int absoluto(int num){
  if (num<0) {
      return num*(-1);
  }else{
      return num ;
  }
}

int main(){
    int n1,i;
    
    for(i=0;i<5;i++){
        printf("\ndigite o numero %d:",i);
        scanf("%d",&n1);
        printf("\n valor absoluto Ã©:%d\n",absoluto(n1));
        
        
    }
    return 0;
}
